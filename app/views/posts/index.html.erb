<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">タイトル</th>
      <th scope="col">作成日</th>
    </tr>
  </thead>
  
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <th scope="row"><%= post.id %></th>
        <td><%= post.content %></td>
        <td><%= post.created_at %></td>
      </tr>
    <% end %>
  </tbody>
  
</table>